    "host": "petstore.swagger.io",
    "basePath": "/v2",
    "paths": {
        "/welcome/{name}": {
            "get": {
                "description": "자세한 설명은 이곳에 적습니다.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "Summary를 적어 줍니다",
                "parameters": [
                    {
                        "type": "string",
                        "description": "User name",
                        "name": "name",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/main.welcomeModel"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "main.welcomeModel": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer",
                    "format": "int64",
                    "example": 1
                },
                "name": {
                    "type": "string",
                    "example": "account name"
                }
            }
        }
    }
